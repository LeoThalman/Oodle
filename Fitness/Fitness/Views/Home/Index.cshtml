@*model iterator used to make example table of students*@
@model IEnumerable<Fitness.Models.Student>
@{
    ViewBag.Title = "Home Page";
}


<div class="container" style="color:black">
    <img src="~/Content/running.jpg" alt="Image Not Found." style="width:100%" />
    <h1 class="top-left">Runner's World</h1>
    <div class="centered"><p class="lead">Runners world is your site to get up to date information on your cross country runs as well as training programs!</p></div>
</div>


<div style="color: black">
    <h2>Record your Runs</h2>
    <p>
        Upload your runs in the form of .fit files to keep track of your progress and have a personalized training
        schedule created for you. Just click the Upload button on the navigation bar to get started.
    </p>
</div>

