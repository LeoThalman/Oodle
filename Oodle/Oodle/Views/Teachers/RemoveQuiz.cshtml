@model Oodle.Models.ViewModels.TeacherVM


<div class="removeNotif">
    <h1>Delete Quiz</h1>
    <p>Click the Delete Quiz button to confirm deleting the quiz, otherwise click the back button to the quiz page</p>
    <p>Quiz To Remove: @Model.quiz.QuizName</p>

    @Html.ActionLink("Back To Quiz", "ViewQuiz", "Teachers", new { QuizID = Model.quiz.QuizID, ClassID = Model.quiz.ClassID }, new { @class = "btn btn-default" })

    @Html.ActionLink("Delete Quiz", "DeleteQuiz", "Teachers", new { QuizID = Model.quiz.QuizID, ClassID = Model.quiz.ClassID }, new { @class = "btn btn-default" })
    <p></p>

</div>